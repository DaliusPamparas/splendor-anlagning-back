{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "machines": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::machine.machine",
      "mappedBy": "users_permissions_users"
    },
    "created_issues": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::issue.issue",
      "mappedBy": "created_by"
    },
    "assigned_issues": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::issue.issue",
      "mappedBy": "assigned_to"
    },
    "language": {
      "type": "string",
      "default": "sv"
    },
    "created_work_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::work-order.work-order",
      "mappedBy": "created_by"
    },
    "assigned_work_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::work-order.work-order",
      "mappedBy": "assigned_to"
    }
  }
}
