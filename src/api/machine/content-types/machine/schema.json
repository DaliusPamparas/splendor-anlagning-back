{
  "kind": "collectionType",
  "collectionName": "machines",
  "info": {
    "singularName": "machine",
    "pluralName": "machines",
    "displayName": "Machine"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "qrid": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "users_permissions_users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "machines"
    },
    "issues": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::issue.issue",
      "mappedBy": "machine"
    },
    "checklists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::checklist.checklist",
      "inversedBy": "assignedMachines"
    },
    "checklistCompletions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::checklist-completion.checklist-completion",
      "mappedBy": "machine"
    },
    "vehicleGroup": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::vehicle-group.vehicle-group",
      "inversedBy": "machines"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "licensePlate": {
      "type": "string",
      "maxLength": 20,
      "default": ""
    },
    "mileageHours": {
      "type": "integer",
      "min": 0
    },
    "mileageType": {
      "type": "enumeration",
      "enum": ["mileage", "hours", "none"],
      "default": "none"
    },
    "year": {
      "type": "integer",
      "min": 1900,
      "max": 2030
    },
    "manufacturer": {
      "type": "string",
      "maxLength": 100
    },
    "model": {
      "type": "string",
      "maxLength": 100
    },
    "maintenance_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::maintenanceorder.maintenanceorder",
      "mappedBy": "machine"
    }
  }
}
